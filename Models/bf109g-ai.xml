<?xml version="1.0"?>

<!-- Messerschmitt Bf 109 G14 by Detlef Faber -->

<PropertyList>

 <path>bf109g-ai.ac</path>
 <offsets>
   <z-m>1.6</z-m>
 </offsets>

<!-- TailDragger -->
   <animation>
      <type>rotate</type>
      <property>velocities/true-airspeed-kt</property>
      <interpolation>
         <entry>
            <ind>0</ind>
            <dep>12</dep>
         </entry>
         <entry>
          <ind>24</ind>
          <dep>12</dep>
         </entry>
         <entry>
            <ind>25</ind>
            <dep>9</dep>
         </entry>
         <entry>
            <ind>30</ind>
            <dep>6</dep>
         </entry>
         <entry>
            <ind>35</ind>
            <dep>4</dep>
         </entry>
         <entry>
            <ind>40</ind>
            <dep>2</dep>
         </entry>
         <entry>
            <ind>45</ind>
            <dep>0</dep>
         </entry>
      </interpolation>
      
       <center>
        <x-m>2</x-m>
        <y-m>0</y-m>
        <z-m>-1.5</z-m>
       </center>
       <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
       </axis>
      
      
   </animation>

<!-- ***** Propeller ***** -->
 <animation>
  <type>select</type>
  <object-name>Prop</object-name>
  <object-name>Propeller1</object-name>
  <condition>
   <less-than>
      <property>velocities/true-airspeed-kt</property>
    <value>45</value>
   </less-than>
  </condition>
 </animation>

