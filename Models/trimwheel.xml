<?xml version="1.0"?>
<PropertyList>

 <path>flapwheel.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Circle</object-name>
  <property>controls/flight/elevator-trim</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-750</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>750</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.001</y-m>
   <z-m>-0.102</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>
 
</PropertyList>


